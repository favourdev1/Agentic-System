# LLM provider
LLM_PROVIDER=gemini
GOOGLE_API_KEY=
GEMINI_MODEL=gemini-1.5-flash

# Optional OpenAI fallback
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# LangSmith tracing
LANGSMITH_API_KEY=
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=agentic-system-local

# Optional service defaults for API-backed tools
DEFAULT_API_TIMEOUT_SECONDS=20

# Bank API Configuration
BANK_API_BASE_URL=http://localhost:8000/api/v2/bank-account/request
BANK_API_AUTH_TOKEN=
BANK_API_SESSION_COOKIE=

# Session persistence
SESSION_STORE_DIR=.agentic_sessions

# Prompt governance
PROMPT_CONFIG_DIR=prompts
PROMPT_VERSION=
